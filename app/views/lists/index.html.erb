<% content_for(:title, "ToDo | Lists") %>
    <br>
    <ul class="collection with-header">
        <li class="collection-header">
            <h2 class="center">Lists</h2><b><p class="center-align list blue-text"><%= link_to "New List" , new_list_path%></p></b></li>
        <% @lists.each do |list|%>
            <li class="collection-item list">
                <%= link_to list.name, list_path(list) %>
            </li>
            <% end %>
    </ul>